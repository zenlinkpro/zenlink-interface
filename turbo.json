{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    ".eslintignore",
    ".eslint.js",
    ".npmrc"
  ],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        ".vercel/**/*",
        ".next/**",
        "!.next/cache/**",
        ".graphclient/**",
        ".mesh/**",
        "dist/**",
        "cache/**",
        "public/**/*"
      ],
      "cache": true
    },
    "graphql:generate": {
      "dependsOn": [],
      "cache": false
    },
    "app#build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**"
      ],
      "env": [
        "APP_URL",
        "SWAP_URL",
        "POOL_URL",
        "REFERRALS_URL",
        "ANALYTICS_URL"
      ]
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "clean": {
      "cache": false
    },
    "lint": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "export": {
      "dependsOn": [
        "build"
      ],
      "outputs": [
        "exports.json"
      ],
      "cache": true
    },
    "node": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "deployments/**"
      ]
    },
    "server": {
      "dependsOn": [
        "^build"
      ]
    }
  }
}
