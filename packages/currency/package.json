{
  "name": "@zenlink-interface/currency",
  "version": "0.0.0",
  "description": "Zenlink Currency",
  "author": "zenlinkpro",
  "license": "LGPL-2.1-or-later",
  "keywords": [
    "zenlink",
    "currency",
    "token",
    "native"
  ],
  "main": "dist/index.js",
  "source": "src/index.ts",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
