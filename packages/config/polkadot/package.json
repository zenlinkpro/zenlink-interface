  },
  "dependencies": {
    "@polkadot/api": "^9.4.2",
    "@zenlink-interface/chain": "workspace:*",
    "@zenlink-types/bifrost": "1.7.5",
    "@pendulum-chain/types": "0.2.3"
  },
  "devDependencies": {
    "@zenlink-interface/typescript-config": "workspace:*",
    "tsup": "^6.7.0",
    "typescript": "4.9.5"
  }
}
