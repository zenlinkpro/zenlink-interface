{
  "name": "@zenlink-interface/polkadot-config",
  "version": "0.0.0",
  "private": true,
  "description": "Zenlink Polkadot Config",
  "author": "zenlinkpro",
  "license": "MIT",
  "keywords": [
    "zenlink-interface",
    "polkadot",
    "config"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "source": "src/index.ts",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
