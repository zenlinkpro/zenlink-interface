{
  "name": "@zenlink-interface/router-config",
  "version": "0.0.0",
  "private": true,
  "description": "Zenlink Interface Router Config",
  "author": "zenlinkpro",
  "license": "MIT",
  "keywords": [
    "zenlink",
    "router",
    "config"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "source": "src/index.ts",
  "typings": "dist/index.d.ts",
