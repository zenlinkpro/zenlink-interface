{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET" }
      ]
    }
  ],
  "rewrites": [],
  "crons": [
    {
      "path": "/api/jobs/stats",
      "schedule": "0 */12 * * *"
    },
    {
      "path": "/api/jobs/broadcaster",
      "schedule": "0 1 * * 4"
    }
  ]
}
